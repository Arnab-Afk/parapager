{
  "manifest_version": 2,
  "name": "Wiki Summary Extension",
  "version": "1.0",
  "description": "Get Wikipedia summary for the current page",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "permissions": ["activeTab"]
}
